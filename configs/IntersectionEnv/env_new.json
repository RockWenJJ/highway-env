{
    "id": "intersection-v1",
    "import_module": "highway_env",
    "observation": {
        "type": "Kinematics",
        "vehicles_count": 8,
        "features": ["presence", "x", "y", "vx", "vy", "cos_h", "sin_h", "index"],
        "features_range": {
            "x": [-25, 25],
            "y": [-25, 25],
            "vx": [-20, 20],
            "vy": [-20, 20],
            "dest_x": [-25, 25],
            "dest_y": [-25, 25]
        },
        "absolute": true,
        "order": "ordered",
        "observe_history": true,
        "history_length": 5
    },
    "reward_speed_range": [0.0, 7.0],
    "destination": "o1",
    "policy_frequency": 15,
    "simulation_frequency": 15,
    "max_vehicle_count": 3,
    "spawn_probability": 0.05,
    "duration": 30,
    "no_speed_reward": false,
    "step_reward": -0.1,
    "collision_reward": -10,
    "high_speed_reward": 1,
    "arrived_reward": 5,
    "initial_seed": 10000
}